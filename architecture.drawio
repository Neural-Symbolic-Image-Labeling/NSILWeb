                </mxCell>
                <mxCell id="AMXna7f9SMDh4iYn8GQR-3" value="1. Simply are unlabeled images and waiting labeled&#10;&#10;2. Pop to labeled image set in each circulation" style="text;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;spacingLeft=4;spacingRight=4;overflow=hidden;points=[[0,0.5],[1,0.5]];portConstraint=eastwest;rotatable=0;fontSize=16;whiteSpace=wrap;" parent="3K2ijmS7LoKxx6wy-EOr-29" vertex="1">
